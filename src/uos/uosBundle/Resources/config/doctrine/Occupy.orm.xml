<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="uos\uosBundle\Entity\Occupy" table="occupy">
    <indexes>
      <index name="studentNo" columns="studentNo"/>
      <index name="roomNo" columns="roomNo"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="studentno" type="string" column="studentNo" length="50" nullable="true"/>
    <field name="hallname" type="string" column="hallName" length="50" nullable="true"/>
    <field name="roomno" type="decimal" column="roomNo" precision="3" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
